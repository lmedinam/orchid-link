@page "/Links/{LinkId?}"
@using OrchidLink.Models

<PageTitle>Links</PageTitle>

<div class="cell small-2"><CampaignList @bind-CurrentCampaign=CurrentCampaign /></div>
<LinkViewer CurrentCampaign=@CurrentCampaign />

@code {
	public Campaign? CurrentCampaign = null;

	[Parameter]
    public string? LinkId { get; set; }
}
